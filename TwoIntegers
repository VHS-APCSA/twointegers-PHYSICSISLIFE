public clas TwoIntegers
{
    private int int1;
    private int int2;
    public TwoIntegers()
    {
        int1 = 0;
        int2 = 0;
    }
    public TwoIntegers(int int1, int int2)
    {
        this.int1 = int1;
        this.int2 = int2;
    }
    public String toArithmetic()
    {
        String arithmetic = "";
        double sum = 0;
        sum = ((int1) + (int2));
        double product = 0;
        product = ((int1) * (int2));
        double difference = 0;
        difference = ((int1) - (int2));
        double quotient = 0;
        quotient = ((int1) / (int2));
        arithmetic = ("Your sum equals \n" + sum + "\nYour product equals \n" + product + "\nYour difference equals \n" + difference + "\nYour quotient equals \n" + quotient);
        return arithmetic;
    }
    public void setInt1(int int1)
    {
        this.int1 = int1;
    }
