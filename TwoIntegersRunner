import java.util.Scanner;
public class TwoIntegersRunner
{
    Scanner input = new Scanner(System.in);
    System.out.println("Enter first integer");
    int int1 = input.nextInt();
    System.out.println("Enter second integer");
    int int2 = input.nextInt();
    TwoIntegers twoint = new TwoIntegers(int1, int2);
    String numberSize = twoint.getNumberSize();
    String arithmetic = twoint.getArithmetic();
    String evenOdd = twoint.getEvenOdd();
    String multiple = twoint.getMultiple();
    System.out.println(numberSize, arithmetic, evenOdd, multiple);
}
